{
  "name": "rishi/gorilla-php-practive",
  "description": "PHP interview coding questions with PHPUnit tests (fill in the TODOs).",
  "type": "project",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:filter": "phpunit --filter"
  }
}